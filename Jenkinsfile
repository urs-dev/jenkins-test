pipeline {
  agent any
  stages {
    stage('setup') {
      steps {
        echo 'Hallo Otto'
      }
    }
    stage('prepare-build') {
      steps {
        echo '... prepare files to run'
      }
    }
    stage('run') {
      steps {
        echo '... now running the tasks'
      }
    }
    stage('report') {
      steps {
        echo '... finish report'
      }
    }
  }
}